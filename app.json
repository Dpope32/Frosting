{
  "expo": {
    "name": "Kaiba",
    "slug": "frosting",
    "scheme": "kaiba-nexus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f1f1f"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": [
      "assets/wallpapers-optimized/*",
      "assets/images/*",
      "assets/sounds/*"
    ],
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/icon.png",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "NSCalendarsUsageDescription": "This app needs to access your calendar to set reminders for birthdays.",
        "NSRemindersUsageDescription": "This app needs to send you notifications about birthdays and events."
      },
      "bundleIdentifier": "com.deedaw22.kaibanexus"
    },
    "android": {
      "package": "com.deedaw22.kaibanexus",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1f1f1f"
      },
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 3000
    },
    "plugins": [
      "expo-router",
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      "expo-splash-screen",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#1f1f1f",
          "sounds": [
            "./assets/sounds/default_sound"
          ]
        }
      ],
      "expo-video",
      "expo-updates",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "22727130-682f-425f-bedd-d8fd9ab0b3e0"
      }
    },
    "owner": "deedaw22"
  }
}
