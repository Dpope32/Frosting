{
  "expo": {
    "name": "frosting",
    "slug": "frosting",
    "scheme": "frosting",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1e1e"
    },
    "assetBundlePatterns": [
      "assets/wallpapers-optimized/*",
      "assets/images/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deedaw22.frosting",
      "icon": {
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "NSCalendarsUsageDescription": "This app needs to access your calendar to set reminders for birthdays.",
        "NSRemindersUsageDescription": "This app needs to send you notifications about birthdays and events."
      }
    },
    "android": {
      "package": "com.deedaw22.frosting",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#1e1e1e",
          "sounds": ["./assets/sounds/default"]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "22727130-682f-425f-bedd-d8fd9ab0b3e0"
      }
    },
    "owner": "deedaw22"
  }
}
