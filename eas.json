{
  "cli": {
    "version": ">= 5.9.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "simulator": false,
        "env": {
          "RUBYOPT": "--disable=gems"
        }
      },
      "cache": {
        "disabled": true
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false,
        "env": {
          "RUBYOPT": "--disable=gems"
        }
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "ios": {
        "distribution": "internal",
        "simulator": false,
        "autoIncrement": true,
        "env": {
          "RUBYOPT": "--disable=gems"
        }
      },
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}
