{
  "name": "kaiba-nexus",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "doctor": "npx expo-doctor --skip-native-config-sync-check",
    "deps-check": "depcheck",
    "sort-imports": "import-sort --write 'src/**/*.{ts,tsx}'",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "web-dev": "node ./scripts/start-web-dev.js",
    "web-dev-clear": "npx expo start --clear --web && node start-web-dev.js",
    "proxy": "node proxyServer.js",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "windows": "react-native run-windows",
    "mac": "react-native run-macos",
    "electron": "electron electron-main.js",
    "build-web": "npx expo export --platform web",
    "build-electron": "electron-builder build --win"
  },
  "jest": {
    "preset": "jest-expo",
    "roots": [
      "<rootDir>/tests"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.10.0",
    "@shopify/flash-list": "1.7.3",
    "@tamagui/animations-react-native": "^1.122.7",
    "@tamagui/config": "^1.122.7",
    "@tamagui/core": "^1.122.7",
    "@tamagui/font-inter": "^1.122.7",
    "@tamagui/lucide-icons": "^1.122.8",
    "@tamagui/react-native-media-driver": "^1.122.7",
    "@tamagui/shorthands": "^1.122.7",
    "@tamagui/tabs": "^1.125.31",
    "@tamagui/themes": "^1.122.7",
    "@tanstack/react-query": "^5.66.6",
    "@types/node": "^22.10.10",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.7.9",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.24.1",
    "expo": "~52.0.46",
    "expo-blur": "~14.0.2",
    "expo-calendar": "^14.0.6",
    "expo-clipboard": "~7.0.1",
    "expo-constants": "~17.0.8",
    "expo-contacts": "~14.0.5",
    "expo-doctor": "^1.12.7",
    "expo-file-system": "^18.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "~7.0.4",
    "expo-media-library": "~17.0.6",
    "expo-notifications": "~0.29.14",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "^4.0.9",
    "expo-updates": "~0.27.4",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-draggable-flatlist": "^4.0.2",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-native-wheel-color-picker": "^1.3.1",
    "tamagui": "^1.122.7",
    "zeego": "^2.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/runtime": "^7.27.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.16",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "@vercel/node": "^5.1.14",
    "electron-builder": "^25.1.8",
    "expo-module-scripts": "^4.0.5",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1"
  },
  "private": true,
  "expo": {
    "doctor": {
      "appConfigFieldsNotSyncedCheck": {
        "enabled": false
      },
      "reactNativeDirectoryCheck": {
        "exclude": [
          "tamagui"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "build": {
    "appId": "com.deedaw22.kaiba_nexus",
    "productName": "Kaiba Nexus",
    "files": [
      "electron-main.js",
      "dist/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "./assets/images/icon.png"
    }
  }
}
